{
  "name": "just-another-chat-client",
  "version": "0.0.0",
  "author": "",
  "description": "An electron-vue project",
  "license": "Private",
  "main": "./client/dist/electron/main.js",
  "scripts": {
    "build": "node client/.electron-vue/build.js && electron-builder -w",
    "build:dir": "node client/.electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node client/.electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node client/.electron-vue/build.js",
    "build:server": "cross-env NODE_ENV=production webpack --config server/webpack.config.js",
    "server": "NODE_ENV=development BABEL_ENV=server nodemon server/index.js",
    "dev": "node client/.electron-vue/dev-runner.js",
    "start": "npm-run-all --parallel dev server",
    "start:prod": "NODE_ENV=production node index.js",
    "db": "mongod --dbpath ./data/db",
    "e2e": "npm run pack && mocha client/test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter client/src server client/test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix client/src server client/test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config client/.electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config client/.electron-vue/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start client/test/unit/karma.conf.js",
    "slate": "rimraf node_modules && yarn",
    "postinstall": "npm run lint:fix",
    "prestart": "yarn",
    "docker:login": "$(aws ecr get-login --no-include-email --region us-east-1)",
    "build:docker": "docker build -t jac2 . && docker tag jac2:latest 812974957690.dkr.ecr.us-east-1.amazonaws.com/jac2:latest",
    "docker:push": "docker push 812974957690.dkr.ecr.us-east-1.amazonaws.com/jac2:latest",
    "deploy:dockerrun": "aws s3 cp dockerrun.aws.json s3://jac2",
    "deploy:docker": "aws elasticbeanstalk create-application-version --application-name jac2-backend --version-label $(git rev-parse HEAD|head -c 6) --source-bundle S3Bucket=jac2,S3Key=dockerrun.aws.json",
    "deploy:eb": "aws elasticbeanstalk update-environment --environment-name jac2Backend-env --version-label $(git rev-parse HEAD|head -c 6)"
  },
  "build": {
    "productName": "JaC2",
    "appId": "org.JustAnotherChatClient.JaC2",
    "directories": {
      "output": "client/build"
    },
    "files": [
      "client/dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "client/build/icons/icon.icns"
    },
    "win": {
      "icon": "client/build/icons/icon.ico"
    },
    "linux": {
      "icon": "client/build/icons"
    }
  },
  "dependencies": {
    "all": "^0.0.0",
    "axios": "^0.16.1",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^3.3.7",
    "bulma": "^0.7.1",
    "bunyan": "^1.8.12",
    "compression": "^1.7.2",
    "config": "^1.30.0",
    "cors": "^2.8.4",
    "disable-scroll": "^0.4.0",
    "electron-titlebar": "^0.0.3",
    "express": "^4.16.3",
    "express-bunyan-logger": "^1.3.3",
    "express-jwt": "^5.3.1",
    "json-loader": "^0.5.7",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.1.5",
    "nedb": "^1.8.0",
    "velocity-animate": "^1.5.2",
    "vue": "^2.3.3",
    "vue-electron": "^1.0.6",
    "vue-router": "^2.5.3",
    "vue2-notify": "^1.4.0",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chai": "^4.0.0",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.7.5",
    "electron-builder": "^20.27.1",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.1",
    "mocha": "^3.0.2",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "require-dir": "^0.3.0",
    "rimraf": "^2.6.2",
    "spectron": "^3.7.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.7.2"
  }
}
